                                                <button onclick="ajaxPag2(this.id)" type="button" id="plus3" class="btn btn-dark" value="1" name="use" title="{{nUse}}">Mas
                                                    <i class="material-icons">add</i>
                                                </button>
                                            </p>
                                        </div>
                                    {{/showUse}}
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="row">

                    <div class="col-sm">
                        <div class="card">
                            <div class="border border-danger">
                                <div class="card-header card-header-text card-header-danger">
