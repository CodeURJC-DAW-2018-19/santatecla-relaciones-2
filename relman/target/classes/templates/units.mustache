                                                    <!-- ModalDeletion -->
                                                    <div class="modal fade" id="delete{{destiny}}{{type}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                        <div class="modal-dialog" role="document">
                                                            <div class="modal-content">
                                                                <div class="modal-body">
                                                                    <div class="form-row">
                                                                        <div class="container text-center">
