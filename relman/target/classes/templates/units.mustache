                                                                            {{/related}}
                                                                        </select>
                                                                    </div>
                                                                    <div class="container text-right">

                                                                        <button type="submit" class="btn btn-success">Aceptar</button>
                                                                        <input type="hidden" name="_csrf" value="{{token}}" />
                                                                </form>
                                                            </div>
                                                        </div>
                                                        <div class="modal-footer"></div>
                                                    </div>
                                                </div>
                                        </div>
                                        <div class="container text-right">
                                            <p>
                                                <button type="button" class="btn btn-success btn-sm btn-round" data-toggle="modal" data-target="#addRelComp">
                                                    <i class="material-icons">add</i>
                                                </button>
                                            </p>
                                        </div>
                                        {{/teacher}}
                                        {{#showComponents}}
                                            <div class="container text-left">
                                                <p>
                                                    <button onclick="ajaxPag2(this.id)" type="button" id="plus2" class="btn btn-dark" value="1" name="components" title="{{nComponents}}">Mas
                                                        <i class="material-icons">add</i>
                                                    </button>
                                                </p>
                                            </div>
